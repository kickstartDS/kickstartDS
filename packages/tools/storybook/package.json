{
  "name": "@kickstartds-internal/storybook",
  "version": "1.5.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "chromatic": "yarn run run-chromatic --build-script-name 'storybook:build'",
    "chromatic-base": "yarn run run-chromatic --build-script-name 'storybook-base:build'",
    "chromatic-blog": "yarn run run-chromatic --build-script-name 'storybook-blog:build'",
    "chromatic-content": "yarn run run-chromatic --build-script-name 'storybook-content:build'",
    "chromatic-form": "yarn run run-chromatic --build-script-name 'storybook-form:build'",
    "run-chromatic": "chromatic --skip '@(renovate/**|dependabot/**)' --exit-zero-on-changes --exit-once-uploaded --project-token $CHROMATIC_PROJECT_TOKEN",
    "storybook": "node index.js",
    "storybook-base:build": "node index.js build --module=base",
    "storybook-blog:build": "node index.js build --module=blog",
    "storybook-content:build": "node index.js build --module=content",
    "storybook-form:build": "node index.js build --module=form",
    "storybook:build": "node index.js build"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "del": "^6.0.0",
    "fast-glob": "^3.2.11",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.1",
    "lazysizes": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-emoji-render": "^1.2.4",
    "svgstore": "^3.0.1",
    "yargs-parser": "^21.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@kickstartds/storybook-addon-component-tokens": "^0.1.5",
    "@storybook/addon-a11y": "6.4.20",
    "@storybook/addon-essentials": "6.4.20",
    "@storybook/cli": "6.4.20",
    "@storybook/core": "6.4.21",
    "@storybook/react": "6.4.21",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@whitespace/storybook-addon-html": "git+https://github.com/lmestel/storybook-addon-html.git#v5.3.0",
    "chromatic": "^6.5.4",
    "storybook-dark-mode": "^1.0.9"
  }
}
