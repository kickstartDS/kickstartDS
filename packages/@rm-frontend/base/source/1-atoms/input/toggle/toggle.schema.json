{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://frontend.ruhmesmeile.com/base/atoms/toggle.schema.json",
  "title": "Toggle Switch",
  "description": "Toggle Switch",
  "type": "object",
  "properties": {
    "id": {
      "title": "ID",
      "description": "Input id",
      "type": "string",
      "default": ""
    },
    "name": {
      "title": "Name",
      "description": "Name",
      "type": "string",
      "default": ""
    },
    "checked": {
      "title": "Checked",
      "description": "Toggle is checked",
      "type": "boolean",
      "default": false
    },
    "disabled": {
      "title": "Disabled",
      "description": "Toggle is disabled",
      "type": "boolean",
      "default": false
    }
  },
  "required": ["id", "name", "checked", "disabled"]
}
