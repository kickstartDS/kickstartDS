{{#> base-section headline=(object content="Content-Navigation" subheader="2 columns")}}
  <div class="content-navigation-wrap content-navigation-wrap--col-2">
    <div class="content-navigation" data-component="base.content-navigation">
      {{#each (slice content-navigation 0 2)}}
        <div class="content-navigation__column">
          {{> molecules-content-navigation-item this }}
        </div>
      {{/each}}
    </div>
  </div>
{{/base-section}}
