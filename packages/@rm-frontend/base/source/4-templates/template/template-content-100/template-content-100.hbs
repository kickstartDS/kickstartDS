{{#> organisms-main }}
  {{#if keyvisual}}
    <div class="page-intro">
      {{try "molecules-keyvisual" keyvisual}}
    </div>
  {{/if}}

  {{> organisms-breadcrumb }}

  {{> organisms-headline content="Template 100" }}

  <div class="template template--content-100">
    {{> organisms-typo3-standard-content id=increment }}
    {{try "organisms-download" id=increment }}
  </div>

  {{#if (module "share-bar")}}
  <div class="template template--content-100">
    <div class="l-section">
      {{try "molecules-share-bar" lang="de" info-url="http://ct.de/-2467514" class="l-main-wrap" }}
    </div>
  </div>
  {{/if}}
{{/organisms-main }}
