{
  "presets": [
    [
      "@babel/preset-react",
      { "runtime": "classic", "pragma": "h", "pragmaFrag": "\"div\"" }
    ],
    "@babel/preset-typescript"
  ],
  "sourceType": "unambiguous",
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/modules",
          {
            "useBuiltIns": "usage"
          }
        ]
      ]
    },
    "es5": {
      "presets": [
        [
          "@babel/env",
          {
            "useBuiltIns": "usage",
            "modules": false,
            "debug": false,
            "corejs": 3
          }
        ]
      ]
    }
  }
}
