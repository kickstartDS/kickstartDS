<header class="main-header main-header-sidebar" data-component="base.header" itemscope id="microdata-org"
  itemprop="publisher author" itemtype="http://schema.org/Organization"
  itemref="microdata-org-address microdata-org-social">
  <div class="main-header__logo-nav">
    {{#if modules.base.molecules.contact-flag-label}}
    {{try "molecules-contact-flag-label" }}
    {{/if}}
    {{> molecules-logo }}

    {{#if modules.base.organisms.header.meta}}
    <div class="nav-meta-container">
      {{> molecules-nav-meta }}
    </div>
    {{/if}}
  </div>
</header>

<div class="main-header-sidebar-spacer"></div>