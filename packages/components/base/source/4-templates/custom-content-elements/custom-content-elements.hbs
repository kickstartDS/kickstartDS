{{#> organisms-main }}

  {{#if keyvisual}}
    <div class="page-intro">
      {{try "molecules-keyvisual" keyvisual}}
    </div>
  {{/if}}

  {{> organisms-breadcrumb }}

  {{> organisms-headline content="Custom Content Elements" }}

  <div class="l-main-wrap">
    <div class="template template--content-70-sidebar-30">
      <div class="template__content">

        {{try "organisms-business-card" id=increment }}
        {{try "organisms-business-card-2-items" id=increment }}
        {{try "organisms-business-card-3-items" id=increment }}

        {{try "molecules-highlight-text" id=increment }}

        {{try "organisms-content-box-1-item" id=increment }}
        {{try "organisms-content-box-3-items" id=increment }}
        {{try "organisms-content-box-4-items" id=increment }}
        {{try "organisms-content-box-5-items" id=increment }}

        {{try "organisms-content-navigation" id=increment }}
        {{try "organisms-content-navigation-2-items" id=increment }}
        {{try "organisms-content-navigation-3-items" id=increment }}

        {{try "molecules-content-visual" id=increment }}

        {{try "organisms-collapsible-box" id=increment }}

        {{try "organisms-content-slider" id=increment }}

        {{try "organisms-teaser-box-1-item" id=increment }}
        {{try "organisms-teaser-box-2-items" id=increment }}
        {{try "organisms-teaser-box-3-items" id=increment }}

        {{try "organisms-gallery-slider" id="galleryslider1" }}

        {{try "organisms-gallery-5-tiles" id=increment }}
        {{try "organisms-gallery-4-tiles" id=increment }}
        {{try "organisms-gallery-3-tiles" id=increment }}

        {{try "organisms-logo-tiles" id=increment }}

        {{try "organisms-mail-download" id=increment }}
      </div>

      <aside class="template__sidebar">

        {{try "organisms-contact-form-basic" id="10" }}

      </aside>
    </div>
  </div>

  <div class="template template--content-100">

    {{try "molecules-highlight-text" id=increment }}

    {{try "organisms-business-card" id=increment }}
    {{try "organisms-business-card-2-items" id=increment }}
    {{try "organisms-business-card-3-items" id=increment }}

    {{try "organisms-content-box-1-item" id=increment }}
    {{try "organisms-content-box-3-items" id=increment }}
    {{try "organisms-content-box-4-items" id=increment }}
    {{try "organisms-content-box-5-items" id=increment }}

    {{try "organisms-content-navigation" id=increment }}
    {{try "organisms-content-navigation-2-items" id=increment }}
    {{try "organisms-content-navigation-3-items" id=increment }}

    {{try "molecules-content-visual" id=increment }}

    {{try "organisms-collapsible-box" id=increment }}

    {{try "organisms-content-slider" id=increment }}

    {{try "organisms-teaser-box-1-item" id=increment }}
    {{try "organisms-teaser-box-2-items" id=increment }}
    {{try "organisms-teaser-box-3-items" id=increment }}

    {{try "organisms-gallery-slider" id=increment }}

    {{try "organisms-gallery-5-tiles" id=increment }}
    {{try "organisms-gallery-4-tiles" id=increment }}
    {{try "organisms-gallery-3-tiles" id=increment }}

    {{try "organisms-logo-tiles" id=increment }}

    {{try "organisms-mail-download" id=increment }}

  </div>

{{/organisms-main }}
