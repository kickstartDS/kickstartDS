<div class="nav-meta__list">
  <ul class="nav-meta__list__item" id="nav-meta__contact" role="contentinfo" aria-label="Schneller Kontakt zu uns">
    {{#if modules.real-estate}}
    <li>
      <a href="{{link.pages-real-estate-shortlist}}">
        {{try "atoms-shortlist-nav-meta-counter" class="nav-meta__icon" count="0"}}
        Merkliste
      </a>
    </li>
    {{/if}}
    <li>
      <a href="tel:00492211234560" title="Unsere Telefon-Nummer">
        <meta itemProp="telephone" content="00492211234560" />
        {{> atoms-icon icon="phone" role="img" className="nav-meta__icon" }}
        +49 (0)221 12 34 56-0
      </a>
    </li>
    <li>
      <a href="mailto:mail@domainname.de" title="Unsere Email-Adresse" itemprop="email">
