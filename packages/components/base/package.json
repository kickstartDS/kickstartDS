{
  "name": "@kickstartds/base",
  "version": "1.5.1",
  "description": "kickstartDS Base Module",
  "homepage": "https://ruhmesmeile.com",
  "bugs": {
    "url": "https://github.com/kickstartDS/kickstartDS/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kickstartDS/kickstartDS.git",
    "directory": "packages/components/base"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "ruhmesmeile GmbH <mail@ruhmesmeile.com> (https://ruhmesmeile.com)",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./source/*": "./source/*",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*/index.d.ts",
      "import": "./lib/*/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "lib/*",
        "lib/*/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "source/**/_container.scss",
    "source/**/*-vars.scss"
  ],
  "scripts": {
    "build": "kdsBundle",
    "schema": "kdsBundle --schema",
    "watch": "kdsBundle --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "@kickstartds/core": "^1.5.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.28.0",
    "focus-visible": "^5.0.2",
    "photoswipe": "^5.3.2",
    "react-markdown": "^8.0.3",
    "vhtml": "^2.2.0"
  },
  "devDependencies": {
    "@kickstartds/bundler": "^1.5.0",
    "@types/react": "^17.0.43",
    "bourbon": "^7.2.0",
    "sass-yiq": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "storybook": {
    "url": "https://master--60832aa30a8f94002193547c.chromatic.com"
  }
}
