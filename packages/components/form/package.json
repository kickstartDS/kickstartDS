{
  "name": "@kickstartds/form",
  "version": "1.5.1",
  "description": "kickstartDS Form Module",
  "homepage": "https://ruhmesmeile.com",
  "bugs": {
    "url": "https://github.com/kickstartDS/kickstartDS/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kickstartDS/kickstartDS.git",
    "directory": "packages/components/form"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "ruhmesmeile GmbH <mail@ruhmesmeile.com> (https://ruhmesmeile.com)",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./source/*": "./source/*",
    "./*Component": {
      "types": "./lib/*Component.d.ts",
      "import": "./lib/*Component.js"
    },
    "./*": "./lib/*",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*Component": [
        "lib/*Component.d.ts"
      ],
      "*": [
        "lib/*",
        "lib/*/index.d.ts"
      ]
    }
  },
  "files": [
    "lib",
    "source/**/_container.scss",
    "source/**/*-vars.scss"
  ],
  "scripts": {
    "build": "kdsBundle",
    "schema": "kdsBundle --schema",
    "watch": "kdsBundle --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "@kickstartds/base": "^1.5.1",
    "@kickstartds/core": "^1.5.0",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@kickstartds/bundler": "^1.5.0",
    "bourbon": "^7.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "storybook": {
    "url": "https://master--60d382053a7c1f00494ae27f.chromatic.com"
  }
}
